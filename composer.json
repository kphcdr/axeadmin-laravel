{
  "name": "kphcdr/axeadmin-laravel",
  "description": "axeadmin-laravel",
  "type": "admin",
  "license": "MIT",
  "homepage": "https://github.com/kphcdr/axeadmin-laravel",
  "authors": [
    {
      "name": "kphcdr",
      "email": "kphcdr@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "admin",
    "rbac",
    "axe"
  ],
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "~5.5 || ~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0 || ~8.0",
    "laravel/laravel": "~5.5 || ~6.0",
    "laravel/browser-kit-testing": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Axe\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Axe\\AxeServiceProvider"
      ]
    }
  }
}
